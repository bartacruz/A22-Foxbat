<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>pedestal.ac</path>

  <animation>
    <object-name>pedestal</object-name>

  </animation>

  <model>
    <path>Instruments/fuel/fuelL.xml</path>
    <offsets>
      <x-m>         0.00134 </x-m>
      <y-m>        -0.03100 </y-m>
      <z-m>        -0.03698 </z-m>
    </offsets>
  </model>

  <model>
    <path>Instruments/fuel/fuelR.xml</path>
    <offsets>
      <x-m>         0.00134 </x-m>
      <y-m>         0.03100 </y-m>
      <z-m>        -0.03698 </z-m>
    </offsets>
  </model>

  <model>
