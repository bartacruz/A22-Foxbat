      <y-m>  0.000 </y-m>
      <z-m> -0.265 </z-m>
      <pitch-deg> 21 </pitch-deg>
    </offsets>
  </model>

  <model>
    <path>Instruments/trim/trim.xml</path>
    <offsets>
      <x-m>  0.000 </x-m>
      <y-m>  0.018 </y-m>
      <z-m> -0.129 </z-m>
    </offsets>
  </model>

